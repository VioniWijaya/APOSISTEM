<div class="flex min-h-screen">
    <!-- Sidebar -->
    <%- include('../admin/sidebarAdm') %>
    
    <!-- Main Content -->
    <div class="flex-1 p-8">
        <header class="flex justify-between items-center border-b pb-4">
            <h2 class="text-2xl font-bold text-green-900">Detail Bibit</h2>
        </header>

        <div class="mt-6 grid grid-cols-1 md:grid-cols-3 gap-6">
            <div class="bg-gray-200 w-full h-48 rounded-lg"></div>
            <div class="col-span-2 space-y-4">
                <label class="block">
                    <span class="text-gray-700">Nama Bibit</span>
                    <input type="text" class="input input-bordered w-full" />
                </label>
                <label class="block">
                    <span class="text-gray-700">Jumlah</span>
                    <input type="number" class="input input-bordered w-full" />
                </label>
                <label class="block">
                    <span class="text-gray-700">Stok Masuk</span>
                    <input type="number" class="input input-bordered w-full" />
                </label>
                <label class="block">
                    <span class="text-gray-700">Stok Keluar</span>
                    <input type="number" class="input input-bordered w-full" />
                </label>
                <label class="block">
                    <span class="text-gray-700">Deskripsi</span>
                    <textarea class="textarea textarea-bordered w-full"></textarea>
                </label>
            </div>
        </div>

        <div class="mt-6 text-right">
            <a href="/admin/editBibit" class="bg-green-800 text-white px-6 py-2 rounded-lg hover:bg-green-600">Edit</a>

        </div>
    </div>
</div>
